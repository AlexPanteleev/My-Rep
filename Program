	print(BuyingUsedCars)
	print(GetTestDrive)
}
}

abstract class Car
{

}
abstract class passenger car
{
	public type // new/used
}
{
	public function GetTestDrive
}
{
	public Brand
	public Year
	public Running
	public EngineSize
	public Price
	public BodyType
	public Photo
{
	print(Brand)
	print(Year)
	print(Running)
	print(EngineSize)
	print(Price)
	print(BodyType)
	print(Photo)
}
	
